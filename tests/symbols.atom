fn unique(t) {
    t.assertFalse("hello" == :hello)
        .assertFalse(:hello == :test)
        .assert(:hello, :hello);
}

fn mapKey(t) {
    let result = {
        :name => "atom",
        "name" => "not atom"
    };

    t.assert("atom", result.get(:name))
        .assertNil(result.get(:test))
        .assert("not atom", result.get("name"));
}

pub fn symbolsTests() {
    return [
        unique,
        mapKey
    ];
}