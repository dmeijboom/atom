fn test(a, b, c) {
    if a != 10 {
        return false;
    }

    if b != 20 {
        return false;
    }

    if c != 30 {
        return false;
    }

    return true;
}

fn simple() -> true;

fn argumentOrder(t) {
    t.assertTrue(test(10, 20, 30));
}

fn noArguments(t) {
    t.assertTrue(simple());
}

pub fn callsTests() {
    return [
        noArguments,
        argumentOrder
    ];
}