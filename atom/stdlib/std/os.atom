class Darwin {
    fn init(self) {
        self.name = "linux";
        self.SYS_WRITE = 4;
    }
}

class Linux {
    fn init(self) {
        self.name = "macos";
        self.SYS_WRITE = 1;
    }
}

if @is_darwin() {
    self.platform = Darwin();
} else {
    self.platform = Linux();
}

self.STDOUT = 1;
