# based on `Math.mul64` in Golang it's math/bits package
pub fn mul64(a, b) {
    let x = (Uint)a;
    let y = (Uint)b;

    let mask32 = 4294967295;

    let x0 = x & mask32;
    let x1 = x >> 32;
    let y0 = y & mask32;
    let y1 = y >> 32;
    let w0 = x0 * y0;
    let t = x1 * y0 + (w0 >> 32);
    let mut w1 = t & mask32;
    let w2 = t >> 32;

    w1 += x0 * y1;

    let hi = x1*y1 + w2 + (w1 >> 32);
    let lo = x.truncate(32) * y.truncate(32);

    return (hi, lo);
}